package project2_4;

public class main {
	public static void main(String[] args) {
		int a;
		double b;
		char d;
		String h; 
		
		a = 1;
		
		if (a > 4) {
			System.out.println(a(2.5, 4, "dsfsdfsdfsdf"));
		} else {
			System.out.println("else");
		}
	}
	
	public static String a(double b, int c, String d) {
		System.out.println(b + c + d);
		System.out.println(d);
		return b+c+d;
	}
}
