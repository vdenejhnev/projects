package project1_1;

public class Main {
	public static void main(String[] args) {
		 double a = 5;
	     double b = 6;
	     double c = 0;	
	     
	     System.out.println(" ");
	     
	     while (b >= 0 || b < 5) {
	    	 System.out.println("b = " + b);
	    	 b--;
	     }
	     
	     for(int i = 0; i < 10; i++) {
	    	 System.out.println("i = " + i);
	     }
	}
}
