package project2_5;

public class Car {
	String name;
	String color;
	double engine_value; 
	
	public Car(String name, String color, double engine_value) {
		this.name = name;
		this.color = color;
		this.engine_value = engine_value;
	}
	
	public void getCar() {
		System.out.println("Name: " + this.name);
	}
}


//Car('BMW', 'grey', 15.6);