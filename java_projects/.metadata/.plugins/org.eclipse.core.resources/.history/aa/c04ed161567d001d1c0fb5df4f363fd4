package project2_3;

public class main {
	public static void main(String[] args) {
		build_blocks(5);
		build_blocks(6);
		build_blocks(10);
		build_blocks(12);
		build_blocks(20);
	}
	
	public static void build_blocks(int N) {
		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				System.out.print("# ");
			}
			System.out.println("");
		}
		System.out.println("");
	}
	
}
